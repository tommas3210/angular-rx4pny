{
  "compilerOptions": {
    "baseUrl": ".",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "es2015",
    "target": "es2015",
    "noImplicitAny": false,
    "outDir": "../../../../dist/appinsights/es2015",
    "rootDir": ".",
    "sourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "removeComments": false,
    "lib": ["es2015", "dom"],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "paths": {
      "ngx-analytics": ["../../../../dist/es2015/index"]
    }
  },
  "files": [
    "ngx-analytics-appinsights.ts"
  ],
  "angularCompilerOptions": {
    "skipTemplateCodegen": true,
    "strictMetadataEmit": true,
    "enableSummariesForJit": true,
    "flatModuleOutFile": "index.js",
    "flatModuleId": "ngx-analytics/appinsights"
  }
}
