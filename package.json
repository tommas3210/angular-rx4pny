{
  "name": "ng-webdpt",
  "version": "2.0.1+STS.1001",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-prod": "ng serve --prod",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "buildCss": "node themeCssBuild",
    "generate-i18n-files": "ngx-translate-extract -i ./src -o ./src/i18n/default-en.json --clean --sort --format namespaced-json",
    "build-prod": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod",
    "doc:build": "compodoc -p src/tsconfig.app.json -d ./doc",
    "doc:serve": "compodoc -s -r 4201 -d ./doc",
    "doc:buildandserve": "compodoc -p ./tsconfig.json -s -r 4201 -d ./doc",
    "framework:packagr": "npm run ng build framework",
    "framework:less": "cpx \"src/app/framework/**/*.less\" \"dist/webdpt/framework\" --verbose",
    "framework:assets": "cpx \"src/app/framework/assets\" \"dist/webdpt/framework/assets\" --verbose",
    "framework:build": "npm run framework:packagr && npm run framework:less && npm run framework:assets",
    "showcase": "ng serve showcase-app --port=4220 --open",
    "showcase-build-prod": "ng build showcase-app --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.4",
