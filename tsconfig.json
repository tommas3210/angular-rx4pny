{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "es2015",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "types": [
      "node"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "baseUrl": "src",
    "paths": {
      "@webdpt/framework": [
        "../projects/webdpt/framework/index"
      ],
      "@webdpt/programs": [
        "../projects/webdpt/programs/index"
      ],
      "@webdpt/analytics": [
        "../projects/webdpt/analytics/index"
      ]
    },
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts"
  ]
}
